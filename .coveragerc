[run]
branch = True
concurrency = 
    greenlet
    thread
omit =
    tests/*
    __init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    if self\.debug
    if settings\.DEBUG
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
